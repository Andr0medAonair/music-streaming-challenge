generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Album {
  id          String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name       String
  artist    String
  songs  String?
  coverUrl    String?
  releaseDate String
  genre       String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}